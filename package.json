{
  "name": "ide-haskell-hasktags",
  "main": "./lib/ide-haskell-hasktags",
  "version": "0.0.12",
  "description": "Hasktags-powered replacement for symbols-view on Haskell projects",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell",
    "hasktags",
    "symbols-view"
  ],
  "activationHooks": [
    "language-haskell:grammar-used"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell-hasktags",
  "license": "MIT",
  "engines": {
    "atom": ">=1.22.0 <2.0.0"
  },
  "dependencies": {
    "atom-select-list": "^0.7.0",
    "etch": "^0.12.7",
    "tslib": "^1.8.1"
  },
  "consumedServices": {
    "ide-haskell-upi": {
      "description": "Uses ide-haskell's unified pluggable interface",
      "versions": {
        "^0.3.0": "consumeUPI"
      }
    }
  },
  "devDependencies": {
    "@types/atom": "^1.22.3",
    "atom-haskell-tslint-rules": "^0.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
